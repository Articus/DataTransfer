{
  "name": "articus/data-transfer",
  "description": "Simple validating hydrator for Zend Expressive",
  "keywords": [
    "hydrator",
    "validator",
    "zend",
    "expressive"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Mogliev"
    }
  ],
  "version": "0.1",
  "require": {
    "php": "^5.6 || ^7.0",
    "doctrine/annotations": "^1.2",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-servicemanager": "^3.1",
    "zendframework/zend-validator": "^2.8",
    "zendframework/zend-cache": "^2.7"
  },
  "suggest": {
    "zendframework/zend-serializer": "It will be required for most Zend Cache adapters to store metadata."
  },
  "require-dev": {
    "codeception/base": "*",
    "phpspec/prophecy": "*"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\DataTransfer\\Sample\\": "tests/_sample"
    }
  },
  "scripts": {
    "test": "codecept run --coverage-xml"
  }
}
